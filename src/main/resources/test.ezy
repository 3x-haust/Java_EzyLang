arr: number[] = [5, 2, 3, 4, 1]
print("정렬 전: ")
for (i: number in arr) {
    print("${i}")
}

sort(arr)

print("\n정렬 후: ")

for (i: number in arr) {
    print("${i}")
}

func sort(arr: number[]): void {
    n: number = arr.length()
    for (i: number in 0 .. n - 2) {
        for (j: number in 0 .. n - i - 2) {
            if (arr[j] >= arr[j + 1]) {
                temp: number = arr[j]
                arr[j] = arr[j + 1]
                arr[j + 1] = temp
            }
        }
    }
}